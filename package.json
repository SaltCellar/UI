    "@types/i18next": "^8.4.3",
    "@types/i18next-xhr-backend": "^1.4.1",
    "@types/jest": "^23.1.1",
    "@types/react": "^16.3.9",
    "@types/react-dom": "^16.0.5",
    "@types/react-helmet": "^5.0.5",
    "@types/react-i18next": "^7.3.2",
    "@types/react-redux": "^6.0.2",
    "@types/react-router-dom": "^4.2.6",
    "@types/webpack": "^4.1.3",
