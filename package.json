    "i18next-xhr-backend": "^1.5.1",
    "jest": "^22.4.3",
    "mini-css-extract-plugin": "^0.4.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-helmet": "^5.2.0",
