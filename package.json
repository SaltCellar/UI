    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.2",
    "ts-loader": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^3.0.4",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
